	\inicio;
		timbre;
	    	clave(G^2) {
            	[F.n, A.b, C.n];
            	[B.r];
        	};
        	timbre;
        	[A2.b, B4.b];
        	rep(3) {
		   	[E4.n, E4.n, F4.n, G4.n ]; 
			timbre;
		};
