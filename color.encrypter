        	[F.P-n, A.P-b, C.P-n];
      	[B.P-r];
   	};
   	timbrePiano;
    	[A2.P-b, B4.P-b];
	rep(3) {
		[E4.P-n, E4.P-n, F4.P-n, G4.P-n ];
